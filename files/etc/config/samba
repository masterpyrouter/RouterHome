
config samba
	option workgroup 'WORKGROUP'
	option homes '0'
	option autoshare '0'
	option name 'RouterNAS'
	option description 'Router'

config sambashare
	option browseable 'yes'
	option users 'root'
	option read_only 'no'
	option guest_ok 'no'
	option create_mask '0777'
	option dir_mask '0777'
	option path '/mnt/sdb1'
	option name '我的共享'

